{
  "name": "sails-memory-restorable",
  "version": "0.10.7",
  "description": "Non-persistent in-memory adapter for Sails.js / Waterline",
  "main": "lib/adapter.js",
  "scripts": {
    "test": "node test/runner.js",
    "prepublish": "npm prune"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/balderdashy/sails-memory.git"
  },
  "keywords": [
    "waterline",
    "database",
    "memory",
    "sails",
    "sails.js",
    "sailsjs"
  ],
  "author": "Mike McNeil",
  "contributors": [
    {
      "name": "Cody Stoltman"
    }
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "lodash": "3.10.1",
    "waterline-criteria": "1.0.1",
    "waterline-errors": "0.10.1",
    "async": "1.5.2",
    "waterline-cursor": "0.0.7"
  },
  "devDependencies": {
    "mocha": "2.5.3",
    "waterline-adapter-tests": "~0.12.1"
  },
  "waterlineAdapter": {
    "waterlineVersion": "~0.10.0",
    "interfaces": [
      "semantic",
      "queryable",
      "associations"
    ],
    "features": [
      "cross-adapter",
      "unique",
      "autoIncrement.sequential"
    ]
  }
}
